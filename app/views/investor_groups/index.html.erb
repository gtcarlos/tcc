<h1>Listing investor_groups</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Area</th>
    <th>Privacy</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @investor_groups.each do |investor_group| %>
  <tr>
    <td><%= investor_group.name %></td>
    <td><%= investor_group.area %></td>
    <td><%= investor_group.privacy %></td>
    <td><%= investor_group.description %></td>
    <td><%= link_to 'Show', investor_group %></td>
    <td><%= link_to 'Edit', edit_investor_group_path(investor_group) %></td>
    <td><%= link_to 'Destroy', investor_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

